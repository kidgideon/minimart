{
  "rewrites": [
    {
      "source": "/products/:id",
      "destination": "https://minimart-backend.vercel.app/product/:id"
    },
    {
      "source": "/",
      "destination": "https://minimart-backend.vercel.app/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
