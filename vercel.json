{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/product/:id",
      "destination": "https://minimart-storefront.vercel.app/:storeId/product/:id"
    },
    {
      "source": "/",
      "destination": "https://minimart-storefront.vercel.app/:storeId"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
